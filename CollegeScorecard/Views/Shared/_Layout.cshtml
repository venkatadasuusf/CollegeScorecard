<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - College Scorecard</title>
    <link rel="stylesheet" href="~/css/site.css" />
    <script type="text/javascript" src="~/js/site.js"></script>
</head>
<body>
    <nav>
        <div class="nav-bar">
            <a asp-area="" asp-controller="Home" asp-action="Index">Home </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Symbols">Colleges List </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Chart">College Scorecard </a><text> | </text>
            <a asp-area="" asp-controller="Home" asp-action="Refresh">Refresh </a><text> | </text>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>ISDS, Muma College of Business, University of South Florida</p>
        </footer>
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>